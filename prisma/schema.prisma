generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Analysis {
  id          Int      @id @default(autoincrement())
  sessionUuid String   @unique
  status      String   @default("DRAFT")
  optionsJson String?
  resultJson  String?
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
